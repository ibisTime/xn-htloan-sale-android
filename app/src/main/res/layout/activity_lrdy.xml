<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">


            <com.cdkj.wzcd.view.MyNormalLayout
                android:id="@+id/myNl_name"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                app:txt_normal_title="客户姓名"></com.cdkj.wzcd.view.MyNormalLayout>

            <com.cdkj.wzcd.view.MyNormalLayout
                android:id="@+id/myNl_code"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                app:txt_normal_title="业务编号"></com.cdkj.wzcd.view.MyNormalLayout>

            <com.cdkj.wzcd.view.MyNormalLayout
                android:id="@+id/myNl_teamName"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                app:txt_normal_title="业务团队"></com.cdkj.wzcd.view.MyNormalLayout>

            <com.cdkj.wzcd.view.MyNormalLayout
                android:id="@+id/myNl_areaName"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                app:txt_normal_title="区域经理"></com.cdkj.wzcd.view.MyNormalLayout>

            <com.cdkj.wzcd.view.MyNormalLayout
                android:id="@+id/myNl_saleUserName"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                app:txt_normal_title="信贷专员"></com.cdkj.wzcd.view.MyNormalLayout>

            <com.cdkj.wzcd.view.MyNormalLayout
                android:id="@+id/myNl_loanAmount"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                app:txt_normal_title="贷款金额"></com.cdkj.wzcd.view.MyNormalLayout>

            <com.cdkj.wzcd.view.MyNormalLayout
                android:id="@+id/myNl_carModel"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                app:txt_normal_title="车辆型号"></com.cdkj.wzcd.view.MyNormalLayout>

            <com.cdkj.wzcd.view.MyNormalLayout
                android:id="@+id/myNl_carFrameNo"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                app:txt_normal_title="车架号"></com.cdkj.wzcd.view.MyNormalLayout>

            <com.cdkj.wzcd.view.MyEditLayout
                android:id="@+id/myEl_carNumber"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                app:txt_input_hint="请输入车牌号码"
                app:txt_input_title="车牌号码"></com.cdkj.wzcd.view.MyEditLayout>

            <com.cdkj.wzcd.view.MyMultipleLayout
                android:id="@+id/myMl_carRegcerti"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                app:title="机动车登记证">

            </com.cdkj.wzcd.view.MyMultipleLayout>

            <com.cdkj.wzcd.view.MyMultipleLayout
                android:id="@+id/myMl_carPd"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                app:title="批单">

            </com.cdkj.wzcd.view.MyMultipleLayout>
            <com.cdkj.wzcd.view.MyMultipleLayout
                android:id="@+id/myMl_carKey"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                app:title="车钥匙">

            </com.cdkj.wzcd.view.MyMultipleLayout>

            <com.cdkj.wzcd.view.MyMultipleLayout
                android:id="@+id/myMl_carBigSmj"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                app:title="大本扫描件">

            </com.cdkj.wzcd.view.MyMultipleLayout>

            <com.cdkj.wzcd.view.MyMultipleLayout
                android:id="@+id/myMl_carXszSmj"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                app:title="车辆行驶证扫描件">

            </com.cdkj.wzcd.view.MyMultipleLayout>

            <com.cdkj.wzcd.view.MyMultipleLayout
                android:id="@+id/myMl_dutyPaidProveSmj"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                app:title="完税证明扫描件">

            </com.cdkj.wzcd.view.MyMultipleLayout>


            <View
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="1" />

            <com.cdkj.wzcd.view.MyConfirmBtn
                android:id="@+id/myCb_confirm"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                app:txt_confirm_content="确认">

            </com.cdkj.wzcd.view.MyConfirmBtn>

        </LinearLayout>
    </ScrollView>
</layout>