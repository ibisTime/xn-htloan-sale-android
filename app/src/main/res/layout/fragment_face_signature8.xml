<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/white"
        android:fillViewport="true">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">

            <com.cdkj.wzcd.view.MyVideoLayout
                android:id="@+id/myVl_bankVideo"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                app:title="银行视频">

            </com.cdkj.wzcd.view.MyVideoLayout>

            <com.cdkj.wzcd.view.MyVideoLayout
                android:id="@+id/myVl_companyVideo"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                app:title="公司视频">

            </com.cdkj.wzcd.view.MyVideoLayout>

            <com.cdkj.wzcd.view.MyVideoLayout
                android:id="@+id/myVl_otherVideo"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                app:title="其他视频">

            </com.cdkj.wzcd.view.MyVideoLayout>


            <com.cdkj.wzcd.view.MyMultipleLayout
                android:id="@+id/myml_yhmqtp"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                app:title="银行面签图片">

            </com.cdkj.wzcd.view.MyMultipleLayout>

            <com.cdkj.wzcd.view.MyMultipleLayout
                android:id="@+id/myml_yhht"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                app:title="银行合同">

            </com.cdkj.wzcd.view.MyMultipleLayout>

            <com.cdkj.wzcd.view.MyMultipleLayout
                android:id="@+id/myml_gsht"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                app:title="公司合同">

            </com.cdkj.wzcd.view.MyMultipleLayout>

            <com.cdkj.wzcd.view.MyMultipleLayout
                android:id="@+id/myml_zjhzsqs"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                app:title="资金划转授权书">

            </com.cdkj.wzcd.view.MyMultipleLayout>

            <com.cdkj.wzcd.view.MyMultipleLayout
                android:id="@+id/myml_qtzl"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                app:title="其他资料">

            </com.cdkj.wzcd.view.MyMultipleLayout>

        </LinearLayout>

    </ScrollView>
</layout>
